6. write(int b)

7. int

8. InputStream in = System.in;
   OutputStream out = System.out;
   try {
       while(in.read()!=-1) {
       out.write(in.read());
       }
    }
    catch (IOException e) {
    	  System.out.println("Reached end of buffer");
    }

